<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".Page1Activity">
    <VideoView
        android:id="@+id/videoView"
        android:layout_width="185dp"
        android:layout_height="110dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/visible_constraint"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/Standard_sub_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="XII - A: Science"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/dot1"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/SubjectName_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:layout_marginBottom="4dp"
            android:text="Mathematics"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/Standard_sub_tv"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/standrad_sub_tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="60dp"
            android:layout_marginBottom="8dp"
            android:text="XI - A: Science"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="parent"

            app:layout_constraintStart_toEndOf="@+id/dot1" />

        <TextView
            android:id="@+id/dot1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp"
            android:text="\u25CF"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/Standard_sub_tv" />

        <RelativeLayout
            android:layout_width="500dp"
            android:layout_height="88dp"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/standrad_sub_tv2">

            <ImageButton
                android:id="@+id/CameraOn"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/background_ovel"
                android:backgroundTint="#AEAEAE"
                android:src="@drawable/video" />

            <ImageButton
                android:id="@+id/CameraOff"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/background_ovel"
                android:backgroundTint="#AEAEAE"
                android:src="@drawable/videooff"
                android:visibility="gone" />

            <ImageButton
                android:id="@+id/callbtn"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginLeft="115dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/oval2"
                android:src="@drawable/phone" />

            <ImageButton
                android:id="@+id/micbtn"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginStart="215dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/background_ovel"
                android:src="@drawable/mic" />
            <ImageButton
                android:id="@+id/mutebtn"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginStart="215dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/background_ovel"
                android:visibility="gone"
                android:src="@drawable/mute" />

            <ImageButton
                android:id="@+id/morebtn"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginStart="450dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/background_ovel"
                android:src="@drawable/more" />

            <TextView
                android:id="@+id/camontextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="60dp"
                android:text="Camera on"
                android:textColor="#636363" />
            <TextView
                android:id="@+id/camofftextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="60dp"
                android:text="Camera off"
                android:visibility="gone"
                android:textColor="#636363" />

            <TextView
                android:id="@+id/leavetextView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="119dp"
                android:layout_marginTop="60dp"
                android:text="Leave"
                android:textColor="#636363" />

            <TextView
                android:id="@+id/micontextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="209dp"
                android:layout_marginTop="60dp"
                android:text="speaker on"
                android:textColor="#636363" />
            <TextView
                android:id="@+id/micofftextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="230dp"
                android:layout_marginTop="60dp"
                android:text="speaker off"
                android:visibility="gone"
                android:textColor="#636363" />

            <TextView
                android:id="@+id/moretextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="458dp"
                android:layout_marginTop="60dp"
                android:text="more"
                android:textColor="#636363" />



        </RelativeLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cons2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="220dp"
            android:layout_marginTop="15dp"
            android:background="#AEAEAE"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/paticipan_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/participents"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="8dp"
                android:text="24"
                android:textColor="@color/white"
                android:textSize="16dp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="@id/paticipan_iv"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/paticipan_iv"
                app:layout_constraintTop_toTopOf="@id/paticipan_iv" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:id="@+id/hidebtn"
            android:layout_width="21dp"
            android:layout_height="258dp"
            android:layout_marginStart="-9dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/oval3"

            android:orientation="vertical"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <RelativeLayout
            android:id="@+id/relativehide"
            android:layout_width="100dp"
            android:layout_height="190dp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="120dp"
            android:layout_marginBottom="86dp"
            app:layout_constraintBottom_toTopOf="@+id/SubjectName_tv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <GridLayout
                android:id="@+id/gridView"
                android:layout_width="90dp"
                android:layout_height="183dp"
                android:layout_margin="9dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="#FFFFFF"
                android:columnCount="2"
                android:rowCount="4"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="0"
                    android:layout_column="0"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/pencil" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="0"
                    android:layout_column="1"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/recordbtn" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="1"
                    android:layout_column="0"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/rectangle" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="1"
                    android:layout_column="1"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/undo" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="2"
                    android:layout_column="1"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/erase" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="3"
                    android:layout_column="0"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/crop" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="3"
                    android:layout_column="1"
                    android:background="@drawable/backgroundsquare"
                    android:src="@drawable/icon_left" />

                <ImageButton
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_row="2"
                    android:backgroundTint="@color/white"
                    android:layout_column="0"
                    android:background="@drawable/backgroundsquare"
                    android:text="1"
                    android:src="@drawable/pencil"
                    />



            </GridLayout>
        </RelativeLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintlayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="15dp"
        android:background="#AEAEAE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/rec"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:text="REC    \u25CF"
            android:textColor="#FF414D"
            android:textSize="16dp"

            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/timings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="11dp"
            android:text="01 : 48 : 25"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/rec"
            app:layout_constraintTop_toTopOf="@id/rec" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/cons_grid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintlayout1"
        tools:ignore="MissingConstraints">


    </androidx.constraintlayout.widget.ConstraintLayout>






</androidx.constraintlayout.widget.ConstraintLayout>